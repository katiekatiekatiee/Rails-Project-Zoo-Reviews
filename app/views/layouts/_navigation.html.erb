
<% if !logged_in? %>
    <%= button_to "Sign in with Facebook", '/auth/facebook' %> 
    <%= link_to 'Sign Up', signup_path%>
    <%= link_to 'Log In', login_path%>
<% else %>
    <%= link_to "Add Review", new_review_path %> | 
    <%= link_to "All Reviews", reviews_path %> |
    <%= link_to "Add a New Zoo", new_zoo_path %> |
    <%= link_to "All Zoos", zoos_path %> |
    <%= link_to "Log Out", logout_path, method: 'delete' %>
<% end%>